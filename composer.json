{
    "name": "alt-three/storage",
    "description": "A Cached Secure Storage Provider For Laravel 5",
    "keywords": ["Storage", "Flysystem", "Alt Three"],
    "license": "MIT",
    "authors": [
        {
            "name": "Alt Three",
            "email": "support@alt-three.com"
        }
    ],
    "require": {
        "php": "^7.1.3",
        "illuminate/contracts": "5.5.*|5.6.*",
        "illuminate/support": "5.5.*|5.6.*",
        "graham-campbell/flysystem": "^5.1",
        "graham-campbell/manager": "^4.1"
    },
    "require-dev": {
        "graham-campbell/analyzer": "^2.1",
        "graham-campbell/testbench": "^5.1",
        "mockery/mockery": "^1.0",
        "phpunit/phpunit": "^6.5|^7.0"
    },
    "autoload": {
        "psr-4": {
            "AltThree\\Storage\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "AltThree\\Tests\\Storage\\": "tests/"
        }
    },
    "config": {
        "preferred-install": "dist"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "4.1-dev"
        },
        "laravel": {
            "providers": [
                "AltThree\\Storage\\StorageServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
